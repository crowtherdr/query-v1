<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <table>
      <thead>
        <tr><td>_oid</td><td>Name</td><td>Nickname</td></tr>
      </thead>
      <tbody>
        <% users.forEach(function (user) {
            %><tr><td><%= user._oid %></td><td><%= user.Name %></td><td><%= user.Nickname %></td></tr><%
          }) %>
      </tbody>
    </table>
  </body>
</html>